{
  "name": "markdown-elearnjs",
  "version": "1.2.5",
  "description": "Simple markdown conversion to elearn.js based HTML and PDF.",
  "main": "out/main.js",
  "typings": "out/main.d.ts",
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "npm run compile && nyc mocha --recursive --colors -u tdd out/test/",
    "prepublishOnly": "tsc -p ./"
  },
  "keywords": [
    "Markdown",
    "elearnjs",
    "elearn.js",
    "html",
    "pdf",
    "digital",
    "paper"
  ],
  "author": "Arne Westphal",
  "repository": "https://github.com/elb-min-uhh/markdown-elearnjs",
  "license": "MIT",
  "dependencies": {
    "html-pdf": "^2.2.0",
    "ncp": "^2.0.0",
    "path": "^0.12.7",
    "showdown": "^1.8.6"
  },
  "devDependencies": {
    "@types/html-pdf": "^2.1.2",
    "@types/mocha": "^5.2.4",
    "@types/ncp": "^2.0.1",
    "@types/node": "^10.3.1",
    "@types/rimraf": "^2.0.2",
    "rimraf": "^2.6.2"
  }
}
