{
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "script"
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "atom": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "strict": "error",
        "no-console": "off",
        "no-var": "warn",
        "semi": "warn"
    }
}
